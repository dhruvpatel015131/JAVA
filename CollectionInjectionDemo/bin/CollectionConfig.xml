<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- bean definitions here -->
	<bean id="queBean" class="collectionInjection.Question">
		<property name="id" value="101"></property> 
		<property name="nm" value="What is Java?"></property>
		<property name="ans">
			<list>
				<value>Java is Programming Language.</value>
				<value>Java is Platform.</value>
				<value>Java is scripting Language.</value>
				<value>Java is database.</value>
			</list>
		</property>
	</bean>

</beans>
